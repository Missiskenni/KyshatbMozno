<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.createworkers.CreateWorkersFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="6dp"
        android:orientation="vertical"
        android:gravity="center">

        <Button
            android:id="@+id/btnCreateAdmin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_create_rest"
            android:padding="5dp"
            android:text="Создать админа ресторана"/>

        <Button
            android:id="@+id/btnCreateDeliverer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/btn_create_rest"
            android:padding="5dp"
            android:text="Создать доставщика"/>

    </LinearLayout>



</RelativeLayout>